package com.aulaJava.exercicio7;

import java.util.Scanner;

public class App 
{
    public static void main( String[] args )
    {
        Scanner sn = new Scanner(System.in);
        
        int numero1, numero2;
        System.out.print("Digite o primeiro número: ");
        numero1 = sn.nextInt();
        System.out.print( "Digite osegundo número: " );
     	numero2 = sn.nextInt();
     	
     	if(numero1 == numero2) {
     		System.out.print( "Igual = true\n" );
     	} else {
     		System.out.print( "Igual = false\n" );
     	}
		if(numero1 != numero2) {
			System.out.print( "Não igual = true\n" );	
		} else {
			System.out.print( "Não igual = false\n" );		
		}
		if(numero1 > numero2) {
			System.out.print( "Maior = true\n" );
		} else {
			System.out.print( "Maior = false\n" );
		}
		if(numero1 < numero2) {
			System.out.print( "Menor = true\n" );
		} else {
			System.out.print( "Menor = false\n" );
		}
		if(numero1 >= numero2) {
			System.out.print( "Maior ou igual = true\n" );
		} else {
			System.out.print( "Maior ou igual = false\n" );	
		}
		if(numero1 <= numero2) {
			System.out.print( "Menor ou igual = true\n" );
		} else {
			System.out.print( "Menor ou igual = false\n" );	
		}
    }
}
